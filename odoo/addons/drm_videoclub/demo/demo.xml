<odoo>
    <data noupdate="1">
        <!-- Categoría -->
        <record id="drm_categoria_id" model="drm_videoclub.categoria">
            <field name="drm_nombre">Acción</field>
        </record>
        <record id="drm_categoria_id_2" model="drm_videoclub.categoria">
            <field name="drm_nombre">Fantástico</field>
        </record>
        <record id="drm_categoria_id_3" model="drm_videoclub.categoria">
            <field name="drm_nombre">Drama</field>
        </record>
        <record id="drm_categoria_id_4" model="drm_videoclub.categoria">
            <field name="drm_nombre">Thriller</field>
        </record>

        <!-- Director -->
        <record id="drm_director_id" model="drm_videoclub.director">
            <field name="drm_nombre">Sam Raimi</field>
            <field name="drm_nacionalidad">Estadounidense</field>
            <field name="drm_fecha_nacimiento">1959-10-23 00:00:00</field>
            <field name="drm_biografia">Es un director de cine estadounidense, reconocido mayormente por su trabajo en la dirección de la primera trilogía del personaje Spider-Man</field>
            <field name="drm_foto"></field>
        </record>
        <record id="drm_director_id_2" model="drm_videoclub.director">
            <field name="drm_nombre">Luc Besson</field>
            <field name="drm_nacionalidad">Francesa</field>
            <field name="drm_fecha_nacimiento">1959-03-18 00:00:00</field>
            <field name="drm_biografia">Director, productor y guionista conocido por sus películas de gran éxito como 'Arthur y los Minimoys'.</field>
            <field name="drm_foto"></field>
        </record>
        <record id="drm_director_id_3" model="drm_videoclub.director">
            <field name="drm_nombre">James Cameron</field>
            <field name="drm_nacionalidad">Canadiense</field>
            <field name="drm_fecha_nacimiento">1954-08-16 00:00:00</field>
            <field name="drm_biografia">Es un director y guionista conocido por su película de acción y ciencia ficción The Terminator.</field>
            <field name="drm_foto"></field>
        </record>
        <record id="drm_director_id_4" model="drm_videoclub.director">
            <field name="drm_nombre">Christopher Nolan</field>
            <field name="drm_nacionalidad">Británico</field>
            <field name="drm_fecha_nacimiento">1970-07-30 00:00:00</field>
            <field name="drm_biografia">Director conocido por su trabajo en la trilogía de 'The Dark Knight' y películas como 'Inception' y 'Dunkirk'.</field>
            <field name="drm_foto"></field>
        </record>

        <!-- Actor -->
        <record id="drm_actor_id" model="drm_videoclub.actor">
            <field name="drm_nombre">Tobey Maguire</field>
            <field name="drm_nacionalidad">Estadounidense</field>
            <field name="drm_fecha_nacimiento">1975-06-27 00:00:00</field>
            <field name="drm_biografia">Es un actor y productor de cine estadounidense, conocido principalmente por interpretar a Peter Parker en la trilogía de Spider-Man de Sam Raimi</field>
            <field name="drm_foto"></field>
        </record>
        <record id="drm_actor_id_2" model="drm_videoclub.actor">
            <field name="drm_nombre">Leonardo DiCaprio</field>
            <field name="drm_nacionalidad">Estadounidense</field>
            <field name="drm_fecha_nacimiento">1974-11-11 00:00:00</field>
            <field name="drm_biografia">Actor y productor conocido por sus papeles en 'Titanic', 'Inception', 'The Revenant', entre otras.</field>
            <field name="drm_foto"></field>
        </record>
        <record id="drm_actor_id_3" model="drm_videoclub.actor">
            <field name="drm_nombre">Freddie Highmore</field>
            <field name="drm_nacionalidad">Británico</field>
            <field name="drm_fecha_nacimiento">1992-02-14 00:00:00</field>
            <field name="drm_biografia">Es un actor y productor británico. Hizo su debut como intérprete en la película de comedia Women Talking Dirty, y desde entonces ha participado en Charlie y la fábrica de chocolate, Arthur y los Minimoys...</field>
            <field name="drm_foto"></field>
        </record>
        <record id="drm_actor_id_4" model="drm_videoclub.actor">
            <field name="drm_nombre">Christian Bale</field>
            <field name="drm_nacionalidad">Estadounidense</field>
            <field name="drm_fecha_nacimiento">1974-01-30 00:00:00</field>
            <field name="drm_biografia">Es un actor y productor estadounidense. Ha participado en El Caballero Oscuro.</field>
            <field name="drm_foto"></field>
        </record>

        <!-- Película -->
        <record id="drm_pelicula_id" model="drm_videoclub.pelicula">
            <field name="drm_nombre">Spider-Man</field>
            <field name="drm_sinopsis">Tras la muerte de sus padres, Peter Parker, un tímido estudiante, vive con su tía May y su tío Ben. Precisamente debido a su retraimiento no es un chico muy popular en el instituto. Un día le muerde una araña que ha sido modificada genéticamente; a la mañana siguiente, descubre estupefacto que posee la fuerza y la agilidad de ese insecto.</field>
            <field name="drm_duracion">121</field>
            <field name="drm_precio">4.95</field>
            <field name="drm_ejemplares">50</field>
            <field name="drm_fecha_salida">2002-06-21 00:00:00</field>
            <field name="drm_portada"></field>
            <!-- Many2Many -->
            <field name="drm_categoria_ids" eval="[(6, 0, [ref('drm_categoria_id'), ref('drm_categoria_id_2')])]"/>
            <field name="drm_actor_ids" eval="[(6, 0, [ref('drm_actor_id')])]"/>
            <!-- Many2one -->
            <field name="drm_director_id" ref="drm_director_id"/>
        </record>
        <record id="drm_pelicula_id_2" model="drm_videoclub.pelicula">
            <field name="drm_nombre">Arthur y los Minimoys</field>
            <field name="drm_sinopsis">Justo cuando su abuela (Mia Farrow) está a punto de perder la casa en la que vive, Arthur (Freddie Highmore), un niño de 10 años, recuerda que su abuelo le había hablado de un gran tesoro oculto en la tierra de los Minimoys, un universo de pequeños seres fantásticos que viven en armonía con la naturaleza, un mundo tan lejano y sin embargo tan cercano e invisible al ojo humano. Decidido a salvar a su abuela, Arthur conseguirá la llave para entrar en él y allí conocerá a la princesa Selenia y a su hermano Betameche. Juntos buscarán el tesoro escondido y deberán enfrentarse al diabólico M, en una aventura que nos enseñará que, en ocasiones, los pequeños héroes son capaces de conseguir grandes logros.</field>
            <field name="drm_duracion">103</field>
            <field name="drm_precio">7.50</field>
            <field name="drm_ejemplares">30</field>
            <field name="drm_fecha_salida">2006-11-29 00:00:00</field>
            <field name="drm_portada"></field>
            <!-- Many2Many -->
            <field name="drm_categoria_ids" eval="[(6, 0, [ref('drm_categoria_id_2')])]"/>
            <field name="drm_actor_ids" eval="[(6, 0, [ref('drm_actor_id_3')])]"/>
            <!-- Many2one -->
            <field name="drm_director_id" ref="drm_director_id_2"/>
        </record>
        <record id="drm_pelicula_id_3" model="drm_videoclub.pelicula">
            <field name="drm_nombre">The Dark Knight</field>
            <field name="drm_sinopsis">Batman debe enfrentarse al Joker, un criminal que quiere destruir Gotham City, mientras lidia con la creciente amenaza del crimen organizado.</field>
            <field name="drm_duracion">152</field>
            <field name="drm_precio">6.00</field>
            <field name="drm_ejemplares">40</field>
            <field name="drm_fecha_salida">2008-07-18 00:00:00</field>
            <field name="drm_portada"></field>
            <!-- Many2Many -->
            <field name="drm_categoria_ids" eval="[(6, 0, [ref('drm_categoria_id'), ref('drm_categoria_id_4')])]"/>
            <field name="drm_actor_ids" eval="[(6, 0, [ref('drm_actor_id_4')])]"/>
            <!-- Many2one -->
            <field name="drm_director_id" ref="drm_director_id_4"/>
        </record>
        <record id="drm_pelicula_id_4" model="drm_videoclub.pelicula">
            <field name="drm_nombre">Titanic</field>
            <field name="drm_sinopsis">Una joven aristócrata se enamora de un pintor mientras viajan a bordo del infame transatlántico Titanic, que acaba hundiéndose en el océano Atlántico.</field>
            <field name="drm_duracion">195</field>
            <field name="drm_precio">4.50</field>
            <field name="drm_ejemplares">60</field>
            <field name="drm_fecha_salida">1997-12-19 00:00:00</field>
            <field name="drm_portada"></field>
            <!-- Many2Many -->
            <field name="drm_categoria_ids" eval="[(6, 0, [ref('drm_categoria_id_3')])]"/>
            <field name="drm_actor_ids" eval="[(6, 0, [ref('drm_actor_id_2')])]"/>
            <!-- Many2one -->
            <field name="drm_director_id" ref="drm_director_id_3"/>
        </record>
    </data>
</odoo>
